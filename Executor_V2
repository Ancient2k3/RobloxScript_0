  table.insert(ignore_inst, ignore_name:lower())
end

if core:FindFirstChild(ui_data.name) then
  core[ui_data.name]:FindFirstChildOfClass("ScreenGui"):Destroy()
else
  ui_data.folder = Instance.new("Folder", core)
  ui_data.folder.Name = ui_data.name
end

repeat task.wait()
until type(ui_data.api_funcs) == "string" and ui_data.api_funcs ~= ""

-- Saved Scripts --
if not isfolder("HHxScripts/Storage") then
  makefolder("HHxScripts/Storage")
  if not isfolder("HHxScripts/Storage/Scripts") then
    makefolder("HHxScripts/Storage/Scripts")
  end
end
-- End --

