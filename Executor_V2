function fill_text_effect(subject, sub_prop, str)
  if not ui_data.vars.loaded_text then
    ui_data.vars.loaded_text = true
    local rslt = (subject[sub_prop] or "") .. " "
    for idx = 1, #str do
      rslt = rslt .. str:sub(idx, idx)
      subject[sub_prop] = rslt
      task.wait(0.05)
    end --ui_data.vars.loaded_text = false
  end
end

function slowly_filling_text(str)
  local rslt = ""
  ui_data.vars.recall = true task.wait(0.5)
  ui_data.vars.recall = false
  for idx = 1, #str do
    rslt = rslt .. str:sub(idx, idx)
    _selectedAPI.Text = "[ " .. rslt .. " ]"
    if ui_data.vars.recall then
      _selectedAPI.Text = "[...]"
      break
    end task.wait(0.05)
  end
end

function str_convert(str)
  if str == "" then return {"", {["Ch·∫£ c√≥ g√¨ h·∫øt..."] = "Tr√≤ ch∆°i n√†y hi·ªán t·∫°i t√¥i ch∆∞a c√≥ h·ªó tr·ª£ h√†m d·ª±ng s·∫µn..."}} end
  local x_codes = string.split(str, "-- APIs Listed --\n")
  local y_info = string.split(x_codes[2], "@\n")
  local z_listed = {}
  for idx = 1, #y_info do
    local witness = string.split(y_info[idx], "~")
    z_listed[witness[1]] = witness[2]
  end
  return {x_codes[1], z_listed}
end

function ui_visible(state, int)
  local x = state or false
  local zp = int or 0
  debug_console.Visible = x
  code_box.Visible = x
  if zp <= 0 then
    exec_one.Visible = x
    exec_loop.Visible = x
    credit_label.Visible = x
    _wait.Visible = x
    _openList.Visible = x
  end
end

function explorer_reset_state()
  explorer_scroll.Visible = false
  debug_console.Size = UDim2.new(0.4, 0, 0.1, 0)
  code_box.Size = UDim2.new(0.4, 0, 0.4, 0)
  _listedAPIs.Size = UDim2.new(0.4, 0, 0.5, 0)
end

function explorer_visible()
  local current_state = explorer_scroll.Visible
  if not current_state then
    if debug_console.Visible then
      debug_console.Size = UDim2.new(0.102, 0, 0.1, 0)
      code_box.Size = UDim2.new(0.102, 0, 0.4, 0)
    elseif _listedAPIs.Visible then
      _listedAPIs.Size = UDim2.new(0.102, 0, 0.5, 0)
    end explorer_scroll.Visible = true
  else
    explorer_reset_state()
  end
end

function api_list_visible()
  local current_state = _listedAPIs.Visible
  if not current_state then
    _listedAPIs.Visible = true
  else
    _listedAPIs.Visible = false
  end ui_visible(current_state, 1)
end

local bif = {
  def = str_convert(ui_data.api_funcs),
  exp = str_convert(ui_data.for_games_api)
}

function run_script()
  local codes, output, o_print, result
  codes = code_box.Text
  output = {}
  o_print = print
  result = ""
  local n_print = function(...)
    table.insert(output, table.concat({...}, " "))
    o_print(...)
  end print = n_print
  local ss, err = pcall(function()
    loadstring(bif.def[1] .. "\n" .. bif.exp[1] .. "\n" .. codes)()
  end) print = o_print
  if ss then result = result .. table.concat(output, "\n")
  else result = result .. err
  end debug_console.Text = tostring(result)
end

local pos_index = 0
local explorer_btn = 0

function add_display_label(str)
  local holder = Instance.new("TextLabel", _listedAPIs)
  holder.Name = "DP:" .. tostring(ui_data.vars.idx_count)
  holder.BackgroundTransparency = 0.1
  holder.BackgroundColor3 = Color3.new(1, 1, 0)
  holder.BorderColor3 = Color3.new(1, 1, 1)
  holder.BorderSizePixel = 1
  holder.Position = UDim2.new(0, 0, pos_index, 0)
  holder.Size = UDim2.new(1, 0, 0.003, 0)
  holder.TextScaled = true
  holder.TextSize = 8
  holder.TextColor3 = Color3.new(0, 0, 0)
  holder.Text = str
  holder.Font = Enum.Font.Arcade
  holder.Visible = true
  pos_index = pos_index + 0.0038
end

function add_info_labels(t)
  if type(t) ~= "table" then funcs.notify("Script hi·ªán ƒëang g·∫∑p l·ªói... !") return end
  for name, desc in pairs(t) do ui_data.vars.idx_count = ui_data.vars.idx_count + 1
    local holder = Instance.new("TextLabel", _listedAPIs)
    holder.Name = "RE:" .. tostring(ui_data.vars.idx_count) .. "_BuiltInFUNC"
    holder.BackgroundTransparency = 1
    holder.BackgroundColor3 = Color3.new(0, 0, 0)
    holder.Position = UDim2.new(0, 0, pos_index, 0)
    holder.Size = UDim2.new(1, 0, 0.003, 0)
    holder.TextScaled = false
    holder.TextSize = 14
    holder.TextColor3 = Color3.new(1, 1, 1)
    holder.Text = name
    holder.TextXAlignment = "Left"
    holder.TextYAlignment = "Top"
    holder.Font = Enum.Font.Code
    holder.Visible = true
    -- Show Info Stuff --
    local info_1 = Instance.new("TextButton", holder)
    info_1.Name = "INFO-FOR:" .. holder.Name
    info_1.BackgroundTransparency = 0.2
    info_1.BackgroundColor3 = Color3.new(1, 1, 0)
    info_1.BorderColor3 = Color3.new(1, 1, 1)
    info_1.Position = UDim2.new(0.89, 0, 0.02, 0)
    info_1.Size = UDim2.new(0.098, 0, 0.98, 0)
    info_1.TextScaled = false
    info_1.TextWrapped = true
    info_1.TextSize = 9
    info_1.TextColor3 = Color3.new(1, 1, 1)
    info_1.TextStrokeColor3 = Color3.new(1, 0, 0)
    info_1.Text = "Info.. ?"
    info_1.Font = Enum.Font.Arcade
    info_1.Visible = true
    -- For Decoration --
    local decor_1 = Instance.new("TextLabel", holder)
    decor_1.Name = "Decoration-On:" .. holder.Name
    decor_1.BackgroundTransparency = 0.2
    decor_1.BackgroundColor3 = Color3.new(1, 1, 0)
    decor_1.BorderColor3 = Color3.new(1, 1, 0)
    decor_1.Position = UDim2.new(0, 0, 0.99, 0)
    decor_1.Size = UDim2.new(0.88, 0, 0.003, 0)
    decor_1.TextScaled = false
    decor_1.TextSize = 9
    decor_1.TextColor3 = Color3.new(1, 1, 1)
    decor_1.Text = ""
    decor_1.Font = Enum.Font.Code
    decor_1.Visible = true
    pos_index = pos_index + 0.0038
    -- Connections --
    info_1.MouseButton1Click:Connect(function()
      _listedAPIs.Size = UDim2.new(0.4, 0, 0.3, 0)
      if not _selectedAPI.Visible then
        _selectedAPI.Visible = true
      end slowly_filling_text(desc)
    end)
  end
end

function clear_inst_label()
  for _, inst in pairs(explorer_scroll:GetChildren()) do
    if inst:IsA("TextLabel") then
      inst:Destroy()
    end
  end
end

function new_table_inst(_path)
  local childs = {}
  if #_path:GetChildren() == 0 then return childs end
  for _, inst in pairs(_path:GetChildren()) do
    local selected_icon = inst_icons[inst.ClassName] or "?"
    childs[inst.Name .. " - " .. selected_icon] = inst
  end return childs
end

function previous_table_inst(_path)
  local previous_childs = {}
  local inst_parent = _path.Parent
  for _, inst in pairs(inst_parent.Parent:GetChildren()) do
    local selected_icon = inst_icons[inst.ClassName] or "?"
    previous_childs[inst.Name .. " - " .. selected_icon] = inst
  end return previous_childs
end

function add_inst_label(t)
  if type(t) ~= "table" then funcs.notify("Script hi·ªán ƒëang g·∫∑p l·ªói... !") return end
  for name_inst, _path in pairs(t) do ui_data.vars.idx_child = ui_data.vars.idx_child + 1
    local path_childs = #_path:GetChildren()
    local info_0, info_1, info_2
    local need_return_yet = nil
    local holder = Instance.new("TextLabel", explorer_scroll)
    holder.Name = "RE:" .. tostring(ui_data.vars.idx_child) .. "_InstSpec"
    holder.BackgroundTransparency = 1
    holder.BackgroundColor3 = Color3.new(0, 0, 0)
    holder.Position = UDim2.new(0, 0, explorer_btn, 0)
    holder.Size = UDim2.new(1, 0, 0.003, 0)
    holder.TextScaled = false
    holder.TextSize = 14
    holder.TextColor3 = Color3.new(1, 1, 1)
    holder.Text = name_inst
    holder.TextXAlignment = "Left"
    holder.TextYAlignment = "Top"
    holder.Font = Enum.Font.Code
    holder.Visible = true
    -- Show Info Stuff --
    if path_childs ~= 0 then
      info_1 = Instance.new("TextButton", holder)
      info_1.Name = "DIG-IN:" .. holder.Name
      info_1.BackgroundTransparency = 0.2
      info_1.BackgroundColor3 = Color3.new(1, 1, 0)
      info_1.BorderColor3 = Color3.new(1, 1, 1)
      info_1.Position = UDim2.new(0.89, 0, 0.02, 0)
      info_1.Size = UDim2.new(0.098, 0, 0.98, 0)
      info_1.TextScaled = false
      info_1.TextWrapped = true
      info_1.TextSize = 9
      info_1.TextColor3 = Color3.new(1, 1, 1)
      info_1.TextStrokeColor3 = Color3.new(1, 0, 0)
      info_1.Text = ">"
      info_1.Font = Enum.Font.Arcade
      info_1.Visible = true
      if not table.find(ignore_inst, _path.Name:lower()) then
        info_0 = Instance.new("TextButton", holder)
        info_0.Name = "GO-BACK:" .. holder.Name
        info_0.BackgroundTransparency = 0.2
        info_0.BackgroundColor3 = Color3.new(1, 1, 0)
        info_0.BorderColor3 = Color3.new(1, 1, 1)
        info_0.Position = UDim2.new(0.779, 0, 0.02, 0)
        info_0.Size = UDim2.new(0.098, 0, 0.98, 0)
        info_0.TextScaled = false
        info_0.TextWrapped = true
        info_0.TextSize = 9
        info_0.TextColor3 = Color3.new(1, 1, 1)
        info_0.TextStrokeColor3 = Color3.new(1, 0, 0)
        info_0.Text = "<"
        info_0.Font = Enum.Font.Arcade
        info_0.Visible = true
        need_return_yet = info_0
        -- Return Button --
        info_0.MouseButton1Click:Connect(function()
          if not ui_data.vars.dig_into then ui_data.vars.dig_into = true
            clear_inst_label() task.wait(0.02)
            explorer_btn = 0 task.wait(0.02)
            add_inst_label(previous_table_inst(_path))
            ui_data.vars.dig_into = false
          else
            funcs.notify("Xin ƒë·ª£i m·ªôt ch√∫t ƒë·ªÉ load l·∫°i c√°c childrens c≈©... ")
          end
        end)
      end
    else
      info_2 = Instance.new("TextButton", holder)
      info_2.Name = "RETURN:" .. holder.Name
      info_2.BackgroundTransparency = 0.2
      info_2.BackgroundColor3 = Color3.new(1, 1, 0)
      info_2.BorderColor3 = Color3.new(1, 1, 1)
      info_2.Position = UDim2.new(0.89, 0, 0.02, 0)
      info_2.Size = UDim2.new(0.098, 0, 0.98, 0)
      info_2.TextScaled = false
      info_2.TextWrapped = true
      info_2.TextSize = 9
      info_2.TextColor3 = Color3.new(1, 1, 1)
      info_2.TextStrokeColor3 = Color3.new(1, 0, 0)
      info_2.Text = "<"
      info_2.Font = Enum.Font.Arcade
      info_2.Visible = true
    end
    -- For Decoration --
    local decor_1 = Instance.new("TextLabel", holder)
    decor_1.Name = "Decoration-On:" .. holder.Name
    decor_1.BackgroundTransparency = 0.2
    decor_1.BackgroundColor3 = Color3.new(1, 1, 0)
    decor_1.BorderColor3 = Color3.new(1, 1, 0)
    decor_1.Position = UDim2.new(0, 0, 0.99, 0)
    decor_1.Size = UDim2.new(0.88, 0, 0.003, 0)
    decor_1.TextScaled = false
    decor_1.TextSize = 9
    decor_1.TextColor3 = Color3.new(1, 1, 1)
    decor_1.Text = ""
    decor_1.Font = Enum.Font.Code
    decor_1.Visible = true
    explorer_btn = explorer_btn + 0.0038
    -- Connections --
    if need_return_yet ~= nil then
      decor_1.Size = UDim2.new(0.765, 0, 0.003, 0)
    end
    if path_childs ~= 0 then
      info_1.MouseButton1Click:Connect(function()
        if not ui_data.vars.dig_into then ui_data.vars.dig_into = true
          clear_inst_label() task.wait(0.02)
          explorer_btn = 0 task.wait(0.02)
          add_inst_label(new_table_inst(_path))
          ui_data.vars.dig_into = false
        else
          funcs.notify("Xin ƒë·ª£i m·ªôt ch√∫t ƒë·ªÉ load h·∫øt c√°c childrens... \n Tr∆∞·ªõc khi ti·∫øp t·ª•c.")
        end
      end)
    else
      info_2.MouseButton1Click:Connect(function()
        if not ui_data.vars.dig_into then ui_data.vars.dig_into = true
          clear_inst_label() task.wait(0.02)
          explorer_btn = 0 task.wait(0.02)
          add_inst_label(previous_table_inst(_path))
          ui_data.vars.dig_into = false
        else
          funcs.notify("Xin ƒë·ª£i m·ªôt ch√∫t ƒë·ªÉ load l·∫°i c√°c childrens tr∆∞·ªõc... ")
        end
      end)
    end
  end
end

tshow_ui.MouseButton1Click:Connect(function()
  if not ui_data.vars.show then
    ui_data.vars.show = true
    tshow_ui.TextColor3 = Color3.new(0, 1, 0)
  else ui_data.vars.show = false
    tshow_ui.TextColor3 = Color3.new(1, 1, 1)
    if _listedAPIs.Visible then
      _listedAPIs.Visible = false
      _selectedAPI.Visible = false
      if _listedAPIs.Size == UDim2.new(0.4, 0, 0.3, 0) then
        _listedAPIs.Size = UDim2.new(0.4, 0, 0.5, 0)
      end
    end explorer_reset_state()
  end ui_visible(ui_data.vars.show) if ui_data.vars.show_api < 5 then
    ui_data.vars.show_api = ui_data.vars.show_api + 1
    if ui_data.vars.show_api >= 2 and ui_data.vars.show_api < 20 then
      fill_text_effect(code_box, "PlaceholderText", " Phi√™n b·∫£n: " .. tostring(ui_data.vars.script_version))
    else code_box.PlaceholderText = "..."
    end
  end
end)
_openList.MouseButton1Click:Connect(api_list_visible)

exec_one.MouseButton1Click:Connect(function()
  exec_one.TextColor3 = Color3.new(0, 1, 0) task.wait(0.02)
  exec_one.TextColor3 = Color3.new(1, 1, 1) run_script()
end)
_selectedAPI.MouseButton1Click:Connect(function()
  _selectedAPI.Visible = false
  if _listedAPIs.Size == UDim2.new(0.4, 0, 0.3, 0) then
    _listedAPIs.Size = UDim2.new(0.4, 0, 0.5, 0)
  end _selectedAPI.Text = "[...]"
end)
exec_loop.MouseButton1Click:Connect(function()
  if not ui_data.vars.looped then
    ui_data.vars.looped = true
    exec_loop.TextColor3 = Color3.new(0, 1, 0)
  else ui_data.vars.looped = false
    exec_loop.TextColor3 = Color3.new(1, 1, 1)
  end while ui_data.vars.looped do wait(ui_data.vars.delay)
    run_script()
  end
end)

_wait.FocusLost:Connect(function(ep)
    if ep and _wait.Text ~= "" then
      if _wait.Text == "clear" then
        code_box.Text = ""
        funcs.notify("ƒê√£ d·ªçn d·∫πp to√†n b·ªô codes... !")
        task.wait(0.02) _wait.Text = ""
      elseif _wait.Text == "api" then
        api_list_visible()
        funcs.notify("B·∫£ng h√†m d·ª±ng s·∫µn... !")
        task.wait(0.02) _wait.Text = ""
      elseif _wait.Text == "explorer" then
        explorer_visible()
        funcs.notify("B·∫£ng Explorer... !")
        task.wait(0.02) _wait.Text = ""
      end if not _wait.Text:match("%d+") then return end
      ui_data.vars.delay = tonumber(_wait.Text) or 0.001
      _wait.Text = ""
    end
end)

add_display_label("H√†m c√≥ s·∫µn: M·∫∑c ƒë·ªãnh.")
add_info_labels(bif.def[2])
add_display_label("H√†m h·ªó tr·ª£: T√πy tr√≤ ch∆°i.")
add_info_labels(bif.exp[2])
add_display_label("Phi√™n b·∫£n: " .. tostring(ui_data.vars.script_version))
add_inst_label(srvs)

funcs.notify("Codes Editor by HoangHienXScripts.")
